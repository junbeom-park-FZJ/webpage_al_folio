	note = {Publisher: American Chemical Society},
	pages = {1522},
	file = {ACS Full Text Snapshot:C\:\\Users\\j.park\\Zotero\\storage\\IEWZX5T4\\acs.cgd.html:text/html;Full Text PDF:C\:\\Users\\j.park\\Zotero\\storage\\W8DDDIPR\\Camara et al. - 2023 - Effect of Low Environmental Pressure on Sintering .pdf:application/pdf},
